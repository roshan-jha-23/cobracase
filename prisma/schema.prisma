

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
enum OrderStatus{
  fulfilled
  shipped
  awaiting_shipment
}
enum  PhoneModel{
  iphonex
  iphone11
  iphone12
  iphone13
  iphone14
  iphone15
  iphone16
}
enum CaseMaterial{
  silicone
  polyCarbonate
}
enum CaseFinish{
  smooth
  texture
}
enum CaseColor{
  black
  blue
  rose
  red
  green
  purple
  yellow
  teal
  orange
  pink
}

model Configuration{
  id String @id @default(cuid())
  width Int
  height Int
  imageUrl String
  model PhoneModel?
  material CaseMaterial?
  finish CaseFinish?
  color CaseColor?
  croppedImageUrl String?
}
